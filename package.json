    "form-data": "^4.0.4",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^8.57.1",
    "eslint-plugin-github": "^4.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "prettier": "^3.6.2",
    "typescript": "^5.1.3"
  }
